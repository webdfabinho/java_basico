
public class TestaCaracteres {
	
	public static void main(String[] args) {
		
		char letra = 'a';
		System.out.println(letra);
		
		char valor = 66;
		System.out.println(valor);//Chama o valor de um caracter
		
		valor = (char) (valor +1);
		System.out.println(valor);//Chama um valor de um caracter + 1
		
		
	}

}
